    <dependencies>
      <dependency id="YUICompressor.NET" version="2.0.0.0" />
      <dependency id="AjaxMin" version="4.48.4489.28432" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\SquishIt.Framework\bin\Release\SquishIt.Framework.dll" target="lib\SquishIt.Framework.dll" />
    <file src="..\SquishIt.Framework\bin\Release\SquishIt.Framework.pdb" target="lib\SquishIt.Framework.pdb" />
    <file src="..\SquishIt.Mvc\bin\Release\SquishIt.Mvc.dll" target="optional\SquishIt.Mvc.dll" />
    <file src="..\SquishIt.Mvc\bin\Release\SquishIt.Mvc.pdb" target="optional\SquishIt.Mvc.pdb" />
  </files>
</package>
